{
  "name": "autosave",
  "main": "./lib/autosave",
  "version": "0.24.4",
  "private": true,
  "description": "Save editors when they lose focus or are closed",
  "repository": "https://github.com/atom/autosave",
  "engines": {
    "atom": ">0.27.0"
  },
  "providedServices": {
    "autosave": {
      "description": "A configuration object to control what is autosaved",
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "dependencies": {
    "fs-plus": "^3.0.0"
  },
  "devDependencies": {
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "spec/fixtures"
    ],
    "globals": [
      "atom",
      "describe",
      "expect",
      "beforeEach",
      "jasmine",
      "waitsForPromise",
      "runs",
      "spyOn",
      "FocusEvent",
      "it"
    ]
  },
  "configSchema": {
    "enabled": {
      "type": "boolean",
      "default": false
    }
  }
}
