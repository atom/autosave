{
  "name": "autosave",
  "main": "./lib/autosave",
  "version": "0.23.2",
  "private": true,
  "description": "Save editors when they lose focus or are closed",
  "repository": "https://github.com/atom/autosave",
  "engines": {
    "atom": ">0.27.0"
  },
  "providedServices": {
    "autosave": {
      "description": "A configuration object to control what is autosaved",
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "dependencies": {
    "fs-plus": "^2.0.0"
  },
  "devDependencies": {
    "coffeelint": "^1.9.7"
  },
  "configSchema": {
    "enabled": {
      "type": "boolean",
      "default": false
    }
  }
}
